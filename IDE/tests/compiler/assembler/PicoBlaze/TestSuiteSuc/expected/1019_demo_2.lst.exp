                   4     ; Simple example of waiting loop
                   5     device kcpsm1
                   6
  00000            7     TEMP1   autoreg
  00001            8     TEMP2   autoreg
  00002            9     TEMP3   autoreg
                  10
                  11     ; Press Start simulation and Animate to run the program
                  12
  00000           13                         ORG       0x000
000 08101         14                         JUMP      Start
                  15     ; Begining of main loop
001 08304         16     Start:              CALL      wait_1s
002 0830E         17                         CALL      wait_100ms
003 08103         18                         JUMP      $                   ; Infinite loop, $ is translated as number of curent line
                  19
                  20
                  21
