                 119     ; UART RX register:
                 120     ;  [1] Rotate leds 8x
                 121     ;  [2] Send "Hello world" via UART
                 122     ;-------------------------------------------------------------------------------------
                 123     RX_resolve          MACRO     uart_byte
                 124
                 125     ;                     RT-IF  uart_byte == 1
                 126     ;                         REPT    8
                 127     ;                         RR      LED_reg
                 128     ;                         wait_for_100ms
                 129     ;                         ENDR
                 130     ;                             EXITM
                 131     ;
                 132     ;                     RT-ELSEIF      uart_byte == 2
                 133     ;                         SendChar  'I'
                 134     ;                         SendChar  'N'
                 135     ;                         SendChar  'T'
                 136     ;                         SendChar  'E'
                 137     ;                         SendChar  'R'
                 138     ;                         SendChar  'R'
                 139     ;                         SendChar  'U'
                 140     ;                         SendChar  'P'
                 141     ;                         SendChar  'T'
                 142     ;                         SendCRLF
                 143     ;                             EXITM
                 144     ;                     #ENDIF
                 145
                 146                         ENDM
