                 257     ;**************************************************************************************
                 258     ; Erase Command - Perform bulk erase of the StrataFLASH memory
                 259     ;**************************************************************************************
                 260     ;
                 261     ; This routine executes the block erase command 128 times with a different base
                 262     ; address in each case.
                 263     ;
                 264     ; Note that this could take as long as 8 minutes 30 seconds
                 265     ; and even typical times will be approximately 2 minutes.
                 266     ;
                 267     ; Registers used s1,s7,s8,s9
                 268     ;
02A 009FE        269     erase_command:          LOAD            s9, #0xfe               ;define base address of block 127 = FE0000
02B 3402D        270                             JUMP            blocks_erase
                 271     ;
                 272     ;
                 273     ;**************************************************************************************
                 274     ; Block Erase Command - Performs erase of lowest 3 blocks of StrataFLASH memory which
                 275     ; covers the address range 000000 to 05FFFF in which the configuration for an XC3S500E
                 276     ; would be able to fit (000000 to 045470).
                 277     ;**************************************************************************************
                 278     ;
                 279     ; This routine executes the block erase command 3 times with a different base
                 280     ; address in each case.
                 281     ;
                 282     ; Each block is 128K bytes and therefore has an address range of 000000 to 01FFFF.
                 283     ; So each block is separated by 020000 hex.
                 284     ;
                 285     ; Registers used s0,s1,s7,s8,s9
                 286     ;
02C 00904        287     block_erase_command:    LOAD            s9, #0x04               ;define base address of block 3 = 040000
02D 30193        288     blocks_erase:           CALL            send_cr
02E 302D4        289                             CALL            send_confirm            ;confirm command with a 'Y' which must be upper case
02F 3011F        290                             CALL            read_from_uart          ;read command character from UART
030 30131        291                             CALL            send_to_uart            ;echo input
031 14F59        292                             COMPARE         uart_data, #_character_y
032 3543F        293                             JUMP            nz, abort_erase
033 30193        294                             CALL            send_cr
034 30209        295                             CALL            send_erase_in_progress
035 30193        296                             CALL            send_cr
036 00800        297                             LOAD            s8, #0x00               ;define lower address of each block = xx0000
037 00700        298                             LOAD            s7, #0x00
038 00F2E        299     blocks_erase_loop:      LOAD            uart_data, #character_fullstop ;progress dots
039 30131        300                             CALL            send_to_uart
03A 30041        301                             CALL            sf_erase_block          ;erase block
03B 1C902        302                             SUB             s9, #0x02               ;decrement base address by 1 block
03C 35C38        303                             JUMP            nc, blocks_erase_loop   ;repeat until block 0 is erased
03D 3022C        304                             CALL            send_ok
03E 34007        305                             JUMP            prompt
                 306     ;
03F 302F1        307     abort_erase:            CALL            send_abort
040 34007        308                             JUMP            prompt
