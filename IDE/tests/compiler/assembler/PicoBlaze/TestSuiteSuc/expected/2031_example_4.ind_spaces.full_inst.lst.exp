1A1 22404        886                             OUTPUT          s4, lcd_output_port
1A2 361A2        887                             CALL            delay_1us
1A3 06401        888                             XOR             s4, #lcd_e              ;E=0
1A4 22404        889                             OUTPUT          s4, lcd_output_port
1A5 24000        890                             RETURN
