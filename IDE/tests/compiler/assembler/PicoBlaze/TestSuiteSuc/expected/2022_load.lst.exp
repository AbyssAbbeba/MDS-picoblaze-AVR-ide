                  96                         JUMP      NZ, wait_1s_i
                  97                         SUB       Temp2, 1
                  98                         JUMP      NZ, wait_1s_i
                  99                         SUB       Temp3, 1
                 100                         JUMP      NZ, wait_1s_i
                 101                         ENDM
